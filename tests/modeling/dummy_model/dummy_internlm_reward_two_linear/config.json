{
  "architectures": [
    "InternLM2ForRewardModel"
  ],
  "attn_implementation": "eager",
  "auto_map": {
    "AutoConfig": "configuration_internlm2.InternLM2RewardConfig",
    "AutoModel": "modeling_internlm2.InternLM2ForRewardModel"
  },
  "bias": false,
  "bos_token_id": 1,
  "eos_token_id": 2,
  "hidden_act": "silu",
